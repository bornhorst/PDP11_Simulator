;PDP11 Assembly Instructions 
;used to generate ascii read in by simulator

OLD:	.WORD	0
NEW:	.WORD	0
FIBO:	.WORD	0
N:	.WORD	6

START:
	INC OLD
	JSR R0, ROUTE1
	BMI ROUTE2	
	

ROUTE1:
	MOV #7, NEW
	MOV FIBO, N
	RTS R0
	BPL ROUTE2

ROUTE2:
	MOV #1, OLD
	MOVB #20,FIBO
	BEQ START
	MOVB OLD, NEW
	BGE ROUTE2

END:	HALT
	.END	START
